<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mappers.plsongMapper">
	<insert id="insert">
		insert into pl_songs values(#{playlistId},#{songId})
	</insert>
	<delete id="delete">
		delete from pl_songs where playlistId=#{playlistId} and songId=#{songId}
	</delete>
</mapper>